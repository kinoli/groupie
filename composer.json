    "type": "craft-plugin",
    "version": "1.0.7",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "groupie"
    ],
    "support": {
        "docs": "https://github.com/kinoli/groupie",
        "issues": "https://github.com/kinoli/groupie/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jesse Knowles",
            "homepage": "http://www.jesseknowles.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "kinoli\\groupie\\": "src/"
        }
    },
    "extra": {
        "name": "Groupie",
        "handle": "groupie",
